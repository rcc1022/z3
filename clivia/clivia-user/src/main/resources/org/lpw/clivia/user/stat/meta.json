    "today": {
        "type": "statistic",
        "props": [{
                "name": "count"
            },
            {
                "name": "register"
            }
        ]
    },
    "line": {
        "type": "line"
    },
    "column": {
        "type": "column"
    },
    "pie": {
        "type": "pie"
    }
}