{
    "key": "clivia.user.stat",
    "uri": "/user/stat/",
    "props": [{
            "name": "date"
        },
        {
            "name": "count"
        },
        {
            "name": "register"
        },
        {
            "name": "online"
        }
    ],
    "query": {
        "type": "grid",
        "search": [{
